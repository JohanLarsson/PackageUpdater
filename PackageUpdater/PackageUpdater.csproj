<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <UseWPF>true</UseWPF>
    <Nullable>enable</Nullable>
    <LangVersion>8</LangVersion>
    <AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
    <CodeAnalysisRuleSet>PackageUpdater.ruleset</CodeAnalysisRuleSet>
    <NoWarn>$(NoWarn);IDE0068</NoWarn>
  </PropertyGroup>

  <PropertyGroup>
    <Authors>Johan Larsson</Authors>
    <Description>Small utility for updating nuget references.</Description>
    <NeutralLanguage>en</NeutralLanguage>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Gu.Analyzers" Version="1.6.6.6-dev" PrivateAssets="all" />
    <PackageReference Include="Gu.Reactive" Version="4.2.0" />
    <PackageReference Include="Gu.Reactive.Analyzers" Version="0.1.3.10" PrivateAssets="all" />
    <PackageReference Include="Gu.Wpf.Reactive" Version="4.2.0" />
    <PackageReference Include="IDisposableAnalyzers" Version="3.2.0" PrivateAssets="all" />
    <PackageReference Include="Microsoft.CodeAnalysis.FxCopAnalyzers" Version="2.9.8" PrivateAssets="all" />
    <PackageReference Include="Ookii.Dialogs" Version="1.0.0" />
    <PackageReference Include="PropertyChangedAnalyzers" PrivateAssets="all" Version="2.7.5" />
    <PackageReference Include="WpfAnalyzers" PrivateAssets="all" Version="2.4.4" />
  </ItemGroup>
</Project>